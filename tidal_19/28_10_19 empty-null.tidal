p 1
  $ whenmod 5 4 (fast 2)
  $ s "<hih>(8,3)"
  # gain 1.2
  # pan (slow 2 sine)

p 2
  $ plyWith 5 (fast 2)
  $ s "noise"
  # speed "<1 0.2 0.8 >"
  # gain 1.1

p 3
  $ slow 2
  $ plyWith 3 (plyWith 10 (|* speed 1.1))
  $ s " ~ jazz"
  # gain 0.4
  # pan (slow 2 cosine)

p 4
  $ slow 2
  $ s "sub:1"
  # cut 1

p 6
  $ every 4 (plyWith 3 (# speed "<0.9 1>"))
  $ plyWith 4 (|* gain 0.9)
  $ s "bd"
  # gain 1.2

p 6
  $ euclid "5" "[8 16]/4"
  $ s "bd"
  # gain 1.2

hush
